{
  "name": "gradtype",
  "version": "1.0.0",
  "description": "Classify user by typing timing",
  "private": true,
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "prepare": "npm run clean && npm run build",
    "lint": "tslint -c tslint.json src/*.ts test/*.ts",
    "mocha": "mocha -r ts-node/register/type-check --reporter spec test/*-test.ts",
    "test": "npm run mocha && npm run lint",
    "survey": "node survey/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/indutny/gradtype.git"
  },
  "keywords": [
    "gradient",
    "descent",
    "typing",
    "timing",
    "classification"
  ],
  "author": "Fedor Indutny <fedor@indutny.com> (http://darksi.de/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/indutny/gradtype/issues"
  },
  "homepage": "https://github.com/indutny/gradtype#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^9.6.48",
    "mocha": "^5.2.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/assert": "0.0.31",
    "@types/debug": "0.0.30",
    "@types/node-fetch": "^2.3.3",
    "debug": "^3.2.6",
    "joi": "^13.7.0",
    "micro": "^9.3.4",
    "node-fetch": "^2.5.0"
  }
}
